{
    "name": "xety/marsbot",
    "description": "A framework to create easyly xat bot.",
    "keywords": ["xat","bot","framework"],
    "homepage": "https://github.com/Xety/MarsBot",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-master",
        "fabpot/php-cs-fixer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Mars\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mars\\Test\\": "tests"
        }
    }
}
